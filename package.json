  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "restart": "git fetch && git pull && pnpm i && pnpm run build && pm2 stop all && pm2 start all",
    "check-deps": "knip",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@logtail/node": "^0.5.5",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/common": "^10.4.20",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.20",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mongoose": "^10.1.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.20",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/websockets": "^10.4.20",
    "@react-email/components": "^0.2.0",
    "agenda": "^5.0.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.56.9",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cli-table3": "^0.6.5",
    "cookie-parser": "^1.4.7",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "ioredis": "^5.7.0",
    "knip": "^5.62.0",
    "mongoose": "^8.17.0",
    "multer": "1.4.5-lts.1",
    "p-queue": "^7.4.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "puppeteer": "^24.15.0",
    "react": "^19.1.1",
    "redis": "^5.6.1",
    "reflect-metadata": "^0.2.2",
    "resend": "^4.7.0",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@nestjs/cli": "^10.4.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.3",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.19.9",
    "@types/passport-jwt": "^4.0.1",
    "eslint": "^8.57.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.15.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "./src/*.{js,jsx,ts,tsx}": [
      "pnpm lint-fix"
    ]
  }
