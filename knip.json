{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": ["src/main.ts"],
  "project": ["**/*.{ts,js}", "ecosystem.config.js"],
  "ignoreDependencies": [
    "lint-staged"
  ],
  "ignoreWorkspaces": [],
  "rules": {
    "dependencies": "error",
    "devDependencies": "error",
    "exports": "error",
    "files": "error",
    "nsExports": "error",
    "types": "error",
    "unlisted": "error"
  }
} 